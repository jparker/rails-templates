!!! XML
!!! 5
%head
  %title= yield :title
  = stylesheet_link_tag 'application'
  = csrf_meta_tag
%body
  .topbar
    .topbar-inner
      .container-fluid
        %h1= yield :title
        = tabs
        = subtabs

  #main
    #flash
      - flash.each do |level, message|
        %p{:class => level}= message

    = yield

  #foot

  = javascript_include_tag 'application'
